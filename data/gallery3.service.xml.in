<?xml version="1.0" encoding="UTF-8"?> 
<service plugin="libgallery3sharing.so" provider="reck"> 
    
    <accounts plugInSetup="1" plugInEdit="1">
      <signup>http://garage.maemo.org/projects/gallery3-sharing-plugin/</signup> 
    </accounts> 
    
    <ui>
        <name>Gallery3</name>
        <icon type="post">@servicesdir@/gallery3-logo.png</icon>
        <icon type="setup">@servicesdir@/gallery3-logo.png</icon>
        <options>
            <option id="album" type="updatable" default="1">
                <caption domain="osso-sharing-ui" key="share_ti_select_album"/>
                <value id="1" key="Gallery" />
            </option>
            <option id="privacy" type="enumeration" default="private">
                <caption domain="osso-sharing-ui" key="share_bd_options_privacy"/>
                <value id="private" domain="osso-sharing-ui" key="share_fi_options_privacy_private"/>
                <value id="public" domain="osso-sharing-ui" key="share_fi_options_privacy_public"/>
            </option>
        </options>
    </ui> 
</service>

